<head>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
</head>
<body>
  <div class="bs-example">
    <div id="myCarousel" class="carousel slide" data-interval="3000" data-ride="carousel">
        <ol class="carousel-indicators">
            <li class="slide-one active"></li>
            <li class="slide-two"></li>
            <li class="slide-three"></li>
        </ol>
        <div class="carousel-inner">
            <div class="active item">
                <img src="http://aicum.org/wp-content/gallery/williams-college/williams_signature_photo.jpg" alt="First Slide">
         		<div class="carousel-caption">
                  <h3>Williams College in the Berkshires</h3>
                </div>
            </div>
            <div class="item">
                <img src="http://www.educational-access.org/wp-content/uploads/2016/12/RS32882_133_Williams_Fall_2011_BARROW-1024x683.jpg" alt="Second Slide">
                <div class="carousel-caption">
                  <h3>Williams College West Hall</h3>
                </div>
            </div>
            <div class="item">
                <img src="https://www.greatvaluecolleges.net/wp-content/uploads/2015/08/williams_sky.jpg" alt="Third Slide">
                <div class="carousel-caption">
                  <h3>Overview of Williams College Campus</h3>
                </div>
            </div>
        </div>
        <a class="carousel-control left">
            <span class="glyphicon glyphicon-chevron-left"></span>
        </a>
        <a class="carousel-control right">
            <span class="glyphicon glyphicon-chevron-right"></span>
        </a>
    </div>
  </div>
</body>
